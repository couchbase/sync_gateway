{
  "disable_persistent_config": true,
  "logging": {
    "console": {
      "log_keys": ["*"]
    }
  },
  "CORS": {
    "Origin":["http://localhost:8000"],
    "LoginOrigin":["http://localhost:8000"],
    "Headers":["Content-Type"],
    "MaxAge": 1728000
  },
  "databases": {
    "db": {
      "server": "couchbase://localhost",
      "username": "username",
      "password": "password",
      "bucket": "default",
      "users": { "GUEST": { "disabled": false, "admin_channels": ["*"] } },
      "allow_conflicts": false,
      "revs_limit": 20
    }
  }
}
