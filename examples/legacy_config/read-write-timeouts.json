{
  "disable_persistent_config": true,
  "ServerReadTimeout": 200,
  "ServerWriteTimeout": 200,
  "ReadHeaderTimeout": 120,
  "IdleTimeout": 180,
  "logging": {
    "console": {
      "log_keys": ["*"]
    }
  },
  "databases": {
    "db": {
      "server": "couchbase://localhost",
      "username": "username",
      "password": "password",
      "bucket": "default",
      "users": { "GUEST": { "disabled": false, "admin_channels": ["*"] } },
      "allow_conflicts": false,
      "revs_limit": 20
    }
  }
}
