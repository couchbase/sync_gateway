{
  "disable_persistent_config": true,
  "databases": {
    "db": {
      "server": "couchbase://localhost",
      "username": "username",
      "password": "password",
      "bucket": "default",
      "import_docs": true,
      "import_filter": `
		function(doc) {
		  if (doc.type != "mobile") {
		    return false
		  }
		  return true
		}
		`,
    }
  }
}
