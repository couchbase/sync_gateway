{
  "databases": {
    "db": {
      "server": "http://localhost:8091",
      "bucket": "default",
      "password": "password",
      "import_docs": "continuous",
      "enable_shared_bucket_access":true,  
      "import_filter": `
		function(doc) {
		  if (doc.type != "mobile") {
		    return false
		  }
		  return true
		}
		`,
    "allow_conflicts": false,
    "revs_limit": 20
    }
  }
}
