{
  "logging": {
    "logFilePath": "/var/tmp/sglogs",
    "console": {
    "logLevel": "debug",
      "logKeys": ["*"]
    },
    "error": {
      "enabled": true,
      "rotation": {
        "maxsize": 20,
        "maxage": 180
      }
    },
    "warn": {
      "enabled": true,
      "rotation": {
        "maxsize": 20,
        "maxage": 90
      }
    },
    "info": {
      "enabled": false
    },
    "debug": {
      "enabled": false
    }
  },
  "databases": {
    "db": {
      "server": "walrus:data",
      "bucket": "default",
      "users": {"GUEST": {"disabled": false,"admin_channels": ["*"]}},
      "allow_conflicts": false,
      "revs_limit": 20
    }
  }
}
