{
  "bootstrap": {
    "server": "couchbases://localhost",
    "server_tls_skip_verify": true,
    "username": "username",
    "password": "password"
  },
  "logging": {
    "console": {
      "enabled": true,
      "log_level": "info",
      "log_keys": ["*"]
    }
  },
  "api": {
    "cors": {
      "origin": ["http://localhost:8000"],
      "login_origin": ["http://localhost:8000"],
      "headers": ["Content-Type"],
      "max_age": 1728000
    }
  }
}